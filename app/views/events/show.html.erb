
<div class="col-md-4">
<p id="notice" style = "color : green"><%= notice %></p>
<table class="table table-bordered table-hover ">
  <thead class="table table-success">
    <tr>
      <th>Code</th>
      <th>Location</th>
      <th>Driver-Id</th>
      <th>Loaded</th>
      <th>equipment_id</th>
      <th colspan="3">Action</th>
    </tr>
    </thead>
  <tbody>
        <tr>
        <td><%= @event.code %></td>
        <td><%= @event.location %></td>
        <td><%= @event.driver_id %></td>
        <td><%= @event.loaded %></td>
        <td><%= @event.equipment.equipment_no %></td>
        <td><%= link_to 'Edit', edit_event_path(@event) %></td>
        <td><%= link_to 'Back', events_path %></td>
      </tr>
  
  </tbody>
</table>
</div>
